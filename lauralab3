<html>
<head>
  <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.css" />
</head>
<body>
  <h1> My Name is Laura Trzaska </h1>
  <h2> This is a map of places I have visited in central Florida </h2>
  
  <div id="mapid" style="height4:00px; width:400px;"></div>
   <script src="http://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.js"></script>
    
<script>
  var mymap = L.map('mapid').setView([29.2021277,-82.0407485], 9);
  
L.tileLayer('http://server.arcgisonline.com/ArcGIS/rest/services/NatGeo_World_Map/MapServer/tile/{z}/{y}/{x}', {
	attribution: 'Tiles &copy; Esri &mdash; National Geographic, Esri, DeLorme, NAVTEQ, UNEP-WCMC, USGS, NASA, ESA, METI, NRCAN, GEBCO, NOAA, iPC',
	maxZoom: 16
 }).addTo(mymap);
  
var markerA = L.marker([29.2021277,-82.0407485]).addTo(mymap)
    .bindPopup("Silver Springs State Park")
    .openPopup();
  var markerB = L.marker([28.8010481,-82.5768147]).addTo(mymap)
    .bindPopup("Homosassa Springs State Park")
    .openPopup();
  
 var markerC = L.marker([27.9874555,-82.8195094]).addTo(mymap)
    .bindPopup("Clearwater Beach")
    .openPopup();
 var markerD = L.marker([28.417663,-81.581212]).addTo(mymap)
    .bindPopup("Walt Disney World")
    .openPopup();
    
 var markerE = L.marker([28.4743207,-81.4678193]).addTo(mymap)
    .bindPopup("Universal Studios")
    .openPopup();
var markerF = L.marker([29.2086838,-81.09661]).addTo(mymap)
    .bindPopup("Daytona Beach")
    .openPopup();
  </script>
  
</body>
</html>
